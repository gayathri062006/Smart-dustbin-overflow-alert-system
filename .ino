#define FRAME_COUNT (sizeof(frames) / sizeof(frames[0]))

// ------------ SETUP ------------
void setup() {
  Serial.begin(9600);

  // LCD
  lcd.init();
  lcd.backlight();

  pinMode(trigPin, OUTPUT);
  pinMode(echoPin, INPUT);
  pinMode(buzzerPin, OUTPUT);
  pinMode(ledPin, OUTPUT);

  // OLED
  display.begin(SSD1306_SWITCHCAPVCC, SCREEN_I2C_ADDR);
  display.clearDisplay();

  lcd.setCursor(0, 0);
  lcd.print("Smart Dustbin");
  lcd.setCursor(0, 1);
  lcd.print("Initializing...");
  delay(2000);
}

// ------------ OLED: draw one frame ------------
void drawFrame(int frame) {
  display.clearDisplay();
  display.drawBitmap(
    (SCREEN_WIDTH  - FRAME_WIDTH)  / 2,
    (SCREEN_HEIGHT - FRAME_HEIGHT) / 2,
    frames[frame],
    FRAME_WIDTH, FRAME_HEIGHT,
    1);
  display.display();
}

// ------------ MAIN LOOP ------------
void loop() {
  // ---- Distance (demo: force overflow) ----
  long duration = 0;
  float distance = 2;      // DEMO: always 2 cm → wasteLevel ~100%

  // REAL SENSOR (later use, now keep commented for sure-overflow demo)
  /*
  digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);

  duration  = pulseIn(echoPin, HIGH);
  distance  = (duration / 2.0) / 29.1;  // cm
  */

  // Dustbin height = 30 cm
  int wasteLevel = map(distance, 30, 0, 0, 100);

  lcd.clear();

  if (wasteLevel < 40) {
    // LOW
    lcd.setCursor(0, 0);
    lcd.print("LEVEL: LOW");
    lcd.setCursor(0, 1);
    lcd.print("Empty: ");
    lcd.print(distance);
    lcd.print("cm");
    digitalWrite(ledPin, LOW);
    noTone(buzzerPin);
  }
  else if (wasteLevel < 70) {
    // MEDIUM
    lcd.setCursor(0, 0);
    lcd.print("LEVEL: MEDIUM");
    lcd.setCursor(0, 1);
    lcd.print("Half Full");
    digitalWrite(ledPin, LOW);
    noTone(buzzerPin);
  }
  else if (wasteLevel < 90) {
    // HIGH
    lcd.setCursor(0, 0);
    lcd.print("LEVEL: HIGH");
    lcd.setCursor(0, 1);
    lcd.print("Fill Soon!");
    digitalWrite(ledPin, HIGH);
    noTone(buzzerPin);
  }
  else {
    // OVERFLOW
    lcd.setCursor(0, 0);
    lcd.print("OVERFLOW!!!");
    lcd.setCursor(0, 1);
    lcd.print("ALERT SENT!");
    digitalWrite(ledPin, HIGH);
    tone(buzzerPin, 1000, 500);
    delay(500);
    Serial.println("MUNICIPALITY ALERT: Dustbin Overflow!");

    // OLED trash bin animation – one cycle
    for (int i = 0; i < FRAME_COUNT; i++) {
      drawFrame(i);
      delay(FRAME_DELAY);
    }
  }

  Serial.print("Distance: ");
  Serial.print(distance);
  Serial.print("cm | Level: ");
  Serial.print(wasteLevel);
  Serial.println("%");

  delay(500);
}
